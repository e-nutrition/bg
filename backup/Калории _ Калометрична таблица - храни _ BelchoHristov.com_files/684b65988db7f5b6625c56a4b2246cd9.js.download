function tbiChangeContainer(){var tbi_label_container=document.getElementsByClassName("tbi-label-container")[0];if(tbi_label_container.style.visibility=='visible'){tbi_label_container.style.visibility='hidden';tbi_label_container.style.opacity=0;tbi_label_container.style.transition='visibility 0s, opacity 0.5s ease'}else{tbi_label_container.style.visibility='visible';tbi_label_container.style.opacity=1}};